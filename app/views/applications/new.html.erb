<h1>New Application</h1>
<%= form_with url: "/applications/:id", method: :post, local: true do |f| %>
  
  <p><%= f.label :applicant_name %>
  <%= f.text_field :applicant_name %></p>

  <p><%= f.label :street_address %>
  <%= f.text_field :street_address %></p>

  <p><%= f.label :city %>
  <%= f.text_field :city %></p>

  <p><%= f.label :state %>
  <%= f.text_field :state %></p>

  <p><%= f.label :zip_code %>
  <%= f.text_field :zip_code %></p>

  <p><%= f.label :description %>
  <%= f.text_field :description %></p>

  <%= f.submit "Submit" %>
<% end %>