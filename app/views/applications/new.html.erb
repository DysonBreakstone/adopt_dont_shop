<h1>New Application</h1>
<%= form_with url: "/applications", method: :post, local: true do |f| %>
  
  <p><%= f.label :name %>
  <%= f.text_field :name %></p>

  <p><%= f.label :street_address %>
  <%= f.text_field :street_address %></p>

  <p><%= f.label :city %>
  <%= f.text_field :city %></p>

  <p><%= f.label :state %>
  <%= f.text_field :state %></p>

  <p><%= f.label :zip_code %>
  <%= f.text_field :zip_code %></p>

  <p><%= f.label :Explain_why_you_would_make_a_good_home_for_this_pet %>
  <%= f.text_field :Explain_why_you_would_make_a_good_home_for_this_pet %></p>

  <%= f.submit "Submit" %>
<% end %>